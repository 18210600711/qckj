<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qcit.dao.LoginLogDao">
	<select id="getLoginLog" parameterType="String" resultType="loginLog">
		select * from loginlog where id=#{id}
	</select>
<insert id="setLoginLog" parameterType="loginLog">
insert into loginlog(id,lastlogintime,lastloginip,authority) value(#{id},now(),#{ip},#{authority})
</insert>
</mapper>